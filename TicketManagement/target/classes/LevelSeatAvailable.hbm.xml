<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="TicketManagement.dao">

	<class name="LevelSeatAvailable" table="SEAT_AVAILABILITY">
		<id name="levelId" column="LEVEL_ID" />
		<property name="rowAvailable" column="AVAILABLE_START_ROW" />
		<property name="seatAvailable" column="AVAILABLE_START_SEAT_NO" />
	</class>

</hibernate-mapping>


